<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.leedh.mapper.PjtMapper">

    <!--프로젝트 등록-->
    <insert id="pjtRegister">

        insert into FL_PROJECT
        values (#{pjtC},
                #{pjtOrgC},
                #{pjtDivC},
                #{pjtStC},
                #{pjtClient},
                #{pjtNm},
                #{pjtEmpList},
                #{pjtPm},
                #{pjtStartDate},
                #{pjtEndDate},
                #{pjtEndYn})
    </insert>


</mapper>